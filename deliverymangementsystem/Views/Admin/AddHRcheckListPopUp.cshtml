                if (hidexpone != '')
                {
                    $("#ddlI9One").find("*").prop("disabled", true);
                }
            }
            else if (hidI9one == 'PRC' || hidI9one == 'EAD') {
                if (hidI9_one_PRC_front != '' && hidI9_one_PRC_back != '' && hidexpone != '') {
                    $("#ddlI9One").find("*").prop("disabled", true);
                }
