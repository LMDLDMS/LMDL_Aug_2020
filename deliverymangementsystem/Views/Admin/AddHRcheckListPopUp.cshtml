        // Gas pin/Mentor activation doc downloade
        $('#gas_pin_menter_activation_code').click(function () {
            if ($('#gas_pin_menter_activation_code').is(':checked') == true) {
                if (confirm('Gas pin/Mentor activation status will be marked as checked. Please confirm')) {
