        var hidI9three = $('#hidI9three').val();
        var hidI9_three_PRC_front = $('#hidI9_three_PRC_front').val();
        var hidI9_three_PRC_back = $('#hidI9_three_PRC_back').val();
        var hidexpthree = $('#hidexpthree').val();
        if (hidI9three != '') {
            $("#ddlI9Three").find("*").prop("disabled", true);
        }

        $('#disablePopup').find("*").prop("disabled", true);

