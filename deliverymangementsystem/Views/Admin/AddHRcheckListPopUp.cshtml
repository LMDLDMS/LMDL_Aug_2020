<script>
    function changecontentone() {
        var ddlone = $('#I9one').val();
        if (ddlone == 'DL' || ddlone == 'USP') {
            $('#dvexpone').show();
            $('#dvfbone').hide();
        }
        else if (ddlone == 'PRC' || ddlone == 'EAD') {
            $('#dvfbone').show();
            $('#dvexpone').show();
        }
        else {
            $('#dvfbone').hide();
            $('#dvexpone').hide();
        }
    }

    function changecontenttwo() {
        var ddlone = $('#I9two').val();
        if (ddlone == 'DL' || ddlone == 'USP') {
            $('#dvexptwo').show();
            $('#dvfbtwo').hide();
        }
        else if (ddlone == 'PRC' || ddlone == 'EAD') {
            $('#dvfbtwo').show();
            $('#dvexptwo').show();
        }
        else {
            $('#dvfbtwo').hide();
            $('#dvexptwo').hide();
        }
    }

    function changecontentthree() {
        var ddlone = $('#I9three').val();
        if (ddlone == 'DL' || ddlone == 'USP') {
            $('#dvexpthree').show();
            $('#dvfbthree').hide();
        }
        else if (ddlone == 'PRC' || ddlone == 'EAD') {
            $('#dvfbthree').show();
            $('#dvexpthree').show();
        }
        else {
            $('#dvfbthree').hide();
            $('#dvexpthree').hide();
        }
    }

    function updateGasPin() {
        if ($('#gaspin').val().trim() == "")
        {
            alert('Please enter valid gas pin.');
        }
        else
        {
            var d_id = @ViewBag.d_id;
            var gaspin = $('#gaspin').val();
                $.ajax({
                    type: 'Get',
                    url: '/Admin/updateHRcheck/' + d_id,
                    data: { "d_id": d_id, "ActionType": "gaspin", "gaspin": gaspin },
                    success: function (data) {
                        //alert(data);
                        alert('Gas pin updated successfully.');
                        window.location.href = "/Admin/AddHRcheckList/" + d_id
                    },
                    error: function (err) {
                    }
                });
        }
    }

    $(document).ready(function () {
        debugger;
        $("#expone").datepicker({ dateFormat: 'mm-dd-yy', changeMonth: true,changeYear: true });
