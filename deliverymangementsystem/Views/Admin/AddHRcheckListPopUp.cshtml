</script>

<div class="container wv-rad5-shadow" id="disablePopup">
    <br />
    <h4 class="tittle-w3-agileits mb-4"> HR Onboarding Check List </h4>
    @using (Html.BeginForm("AddHRcheckListPopUp", "Admin", FormMethod.Get, new { @databvalidatorvalidate = "", @databvalidatortheme = "bs3form", enctype = "multipart/form-data" }))
    {
        <div class="text-center" style="margin-bottom:10px;"><label>Driver - @ViewBag.DAname</label></div>

        <input type="hidden" name="d_id" value="@ViewBag.d_id" id="d_id" />
        
        <div class="card">
            <div class="card-header">
                <b> HR Points </b>
            </div>
            <div class="card-body">

                <div class="row">

                    <div class="col-md-8">
                        <div class="row">
                            <div class="col-md-6">
                                <label>Offer Letter - Eversign</label>
                            </div>
                            <div class="col-md-6">
                                @{
                                    if (ViewBag.chkOffEvrSent == "Yes")
                                    {
                                        <input type="checkbox" checked name="chkOffEvrSent" id="chkOffEvrSent" value="Sent" disabled />
                                    }
                                    else
                                    {
                                        <input type="checkbox" name="chkOffEvrSent" id="chkOffEvrSent" value="Sent" />
                                    }
                                }

                                <label> Sent </label>

                                @{
                                    if (ViewBag.off_letter_evr_signed == "Yes")
                                    {
                                        <input type="checkbox" checked name="chkOffEvrSigned" id="chkOffEvrSigned" value="Signed" disabled />
                                    }
                                    else
                                    {
                                        <input type="checkbox" name="chkOffEvrSigned" id="chkOffEvrSigned" value="Signed" />
                                    }
                                }

                                <label> Signed </label>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-4">
                        <div class="row">
                            <div class="col-md-6">
                                <label>Gas Pin</label>
                            </div>
                            <div class="col-md-6">
                                <input type="text" placeholder="gas pin" maxlength="6" class="form-control" name="gaspin" id="gaspin" value="@ViewBag.gaspin" />
                            </div>
                        </div>
                    </div>

                </div>

                <label>Downloaded I9 documents</label>

                <div class="row" id="ddlI9One">

                    <div class="col-md-4">
                        <input type="hidden" class="form-control" id="hidI9one" name="hidI9one" value="@ViewBag.hidI9one">
                        <select class="form-control" name="I9one" id="I9one" onchange="changecontentone();">
                            <option value="USP">US Passport</option>
                            <option value="PRC">Permanant Resident Card</option>
                            <option value="EAD">EAD</option>
                            <option value="DL" selected>Driver License</option>
                            <option value="SSA">SSA Card</option>
                            <option value="BC">Birth Certificate</option>
                            <option value="Other">Other</option>
                        </select>
                    </div>

                    <div class="col-md-3" id="dvfbone">
                        @{
                            <input type="hidden" class="form-control" id="hidI9_one_PRC_front" name="hidI9_one_PRC_front" value="@ViewBag.I9_one_PRC_front">
                            if (ViewBag.I9_one_PRC_front == "Front")
                            {
                                <input type="checkbox" checked name="chkonefront" value="Front" />
                            }
                            else
                            {
                                <input type="checkbox" name="chkonefront" value="Front" />
                            }
                        }
                        <label> Front </label>
                        @{
                            <input type="hidden" class="form-control" id="hidI9_one_PRC_back" name="hidI9_one_PRC_back" value="@ViewBag.I9_one_PRC_back">
                            if (ViewBag.I9_one_PRC_back == "Back")
                            {
                                <input type="checkbox" checked name="chkoneBack" value="Back" />
                            }
                            else
                            {
                                <input type="checkbox" name="chkoneBack" value="Back" />
                            }
                        }
                        <label> Back </label>
                    </div>

                    <div class="col-md-4" id="dvexpone">
                        @{
                            var dt = ViewBag.one_Expiry_date;
                            if (dt != null) { dt = dt.ToString().Split(' ')[0]; }
                            <input type="hidden" class="form-control" id="hidexpone" name="hidexpone" value="@dt">
                            <input type="text" name="expone" id="expone" placeholder="expiry date" class="form-control" value=@dt required />
                        }
                    </div>

                </div>

                <div class="row" id="ddlI9Two">
                    <div class="col-md-4">
                        <input type="hidden" class="form-control" id="hidI9two" name="hidI9two" value="@ViewBag.hidI9two">
                        <select class="form-control" name="I9two" id="I9two" onchange="changecontenttwo();">
                            <option value="USP">US Passport</option>
                            <option value="PRC">Permanant Resident Card</option>
                            <option value="EAD">EAD</option>
                            <option value="DL">Driver License</option>
                            <option value="SSA" selected>SSA Card</option>
                            <option value="BC">Birth Certificate</option>
                            <option value="Other">Other</option>
                        </select>
                    </div>

                    <div class="col-md-3" id="dvfbtwo">
                        @{
                            <input type="hidden" class="form-control" id="hidI9_two_PRC_front" name="hidI9_two_PRC_front" value="@ViewBag.I9_two_PRC_front">
                            if (ViewBag.I9_two_PRC_front == "Front")
                            {
                                <input type="checkbox" checked name="chktwofront" value="Front" />
                            }
                            else
                            {
                                <input type="checkbox" name="chktwofront" value="Front" />
                            }
                        }
                        <label> Front </label>
                        @{
                            <input type="hidden" class="form-control" id="hidI9_two_PRC_back" name="hidI9_two_PRC_back" value="@ViewBag.I9_two_PRC_back">
                            if (ViewBag.I9_two_PRC_back == "Back")
                            {
                                <input type="checkbox" checked name="chktwoback" value="Back" />
                            }
                            else
                            {
                                <input type="checkbox" name="chktwoback" value="Back" />
                            }
                        }

                        <label> Back </label>
                    </div>

                    <div class="col-md-4" id="dvexptwo">
                        @{
                            var dt2 = ViewBag.two_Expiry_date;
                            if (dt2 != null) { dt2 = dt2.ToString().Split(' ')[0]; }
                            <input type="hidden" class="form-control" id="hidexptwo" name="hidexptwo" value="@dt">
                            <input type="text" name="exptwo" id="exptwo" placeholder="expiry date" class="form-control" value="@dt2" required />
                        }
                    </div>

                </div>

                <div class="row" id="ddlI9Three">

                    <div class="col-md-4">
                        <input type="hidden" class="form-control" id="hidI9three" name="hidI9three" value="@ViewBag.hidI9three">
                        <select class="form-control" name="I9three" id="I9three" onchange="changecontentthree();">
                            <option value="USP" selected>US Passport</option>
                            <option value="PRC">Permanant Resident Card</option>
                            <option value="EAD">EAD</option>
                            <option value="DL">Driver License</option>
                            <option value="SSA">SSA Card</option>
                            <option value="BC">Birth Certificate</option>
                            <option value="Other">Other</option>
                        </select>
                    </div>

