                        <div class="col-lg-4 col-md-4 col-sm-12">
                            <label>JOB TITLE</label>
                            <input type="text" class="form-control" id="jobtitle3" name="jobtitle3" value="@Model.jobtitle3">
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-12">
                            <label>WORK PHONE</label>
                            <input type="text" class="form-control" id="workphone3" name="workphone3" maxlength="10" value="@Model.work_phone3">
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-12">
                            <label>START DATE</label>
                            <input type="text" class="form-control" id="empstartdate3" name="empstartdate3" value="@Model.empstartdate3">
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-12">
                            <label>END DATE</label>
                            <input type="text" class="form-control" id="empenddate3" name="empenddate3" value="@Model.empenddate3">
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-12">
                            <label>ENDING PAY RATE</label>
                            <input type="number" class="form-control" databvalidator="number" id="endpayrate3" name="endpayrate3" value="@Model.ending_pay_rate3">
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-3">
                            <label>ADDRESS</label>
                            <input type="text" class="form-control" id="address3" name="address3" value="@Model.address3">
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-12">
                            <label>CITY</label>
                            <input type="text" class="form-control" id="city3" name="city3" value="@Model.city3">
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-12">
                            <label>STATE</label>
                            <input type="text" class="form-control" id="city3" name="state3" value="@Model.state3">
                        </div>
                        <div class="col-lg-2 col-md-2 col-sm-12">
                            <label>ZIP</label>
                            <input type="text" class="form-control" id="zip3" name="zip3" value="@Model.zip3">
                        </div>



                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <label>Work Performed and Reason for Leaving</label>

                            <textarea type="text" name="WorkPerformed3" rows="3">@Model.WorkPerformed3</textarea>
                        </div>

                        <br />
                    </div>



                </div>

            </div>


            <!-- SIGNATURE DISCLAIMER -->
            <div class="container-fluid wv-rad5-shadow">
                <h3 class="popup-header">SIGNATURE DISCLAIMER</h3>
                <p>
                    I certify that my answers are true and complete to the best of my knowledge.
                    If this application leads to employment, I understand that false or misleading information in my application or interview
                    may result in my release.
                </p>
                <div class="form-group row">
                    <div class="col-sm-6">
                        <label class="control-label" for="dob">Name:</label>
                        <div>
                            <input type="text" class="form-control" id="dob" value="@Model.sig_name" name="sig_name" disabled>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <label class="control-label" for="mobile">Date:</label>
                        <div>
                            <input type="text" class="form-control" id="sig_date" value="@Model.sig_date.ToString().Split(' ')[0]" name="sig_date" disabled>
                        </div>
                    </div>
                </div>
                <div class="row">

                    <div class="col-sm-4">
                        <label>Drv. Lic</label>
                        <img id="profile" alt="No Lic image Found" src="~/Uploads/@Model.upload_linces_photo" width="100" height="100" />
                        @*<input type="hidden" name="upload_linces_photo" id="upload_linces_photo" value="@Model.upload_linces_photo" />*@
                    </div>

                    <div class="col-sm-8" style="margin-top:20px;">
                        <input type="file" id="file" name="linc_file" />
                        @*<input type="button" id="btnclick" value="Upload Photo" class="btn btn-primary" />*@
                        @*<button id="btnclick" class="btn btn-primary">Upload Photo</button>*@
                    </div>

                </div>


                <div class="row">
                    Notes :
                    <textarea name="ApplicantRemarks" id="ApplicantRemarks" class="form-control"></textarea>
                </div>
                <br />


                @*<input type="submit" style="background-color:#5F3E17;color: #fff;font-weight: 800;" class="btn btn-primary" value="Update" />*@
                <button style="color: #fff;font-weight: 800;" class="btn btn-primary">
                    Update
                </button>
                @ViewBag.msg



            </div>
        </div>
</div>



