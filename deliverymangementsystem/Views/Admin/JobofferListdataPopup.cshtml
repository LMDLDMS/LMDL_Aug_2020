<style type="text/css">
    @@font-face {
        font-family: 'Montserrat-Bold';
        src: url('../../fonts/Montserrat-Bold.otf') format('opentype');
    }

    @@font-face {
        font-family: 'Montserrat-Regular';
        src: url('../../fonts/Montserrat-Regular.otf') format('opentype');
    }

    .form-horizontal .control-label {
        text-align: left;
    }

    h3 {
        font-family: Montserrat-Bold;
        font-size: 24px;
    }

    /* Chrome, Safari, Edge, Opera */
    input::-webkit-outer-spin-button,
    input::-webkit-inner-spin-button {
        -webkit-appearance: none;
        margin: 0;
        width: 100%;
        padding: 5px;
        margin: 3px 0 7px 0;
        display: inline-block;
        border: none;
        background: #f1f1f1;
        font-size: 16px;
    }

    /* Firefox */
    input[type=number] {
        -moz-appearance: textfield;
    }

    .row {
        margin-top: 20px;
    }

    /* Full-width input fields */
    input[type=text], input[type=password], input[type=file], input[type=date], input[type=email], select[type=text], textarea[type=text] {
        width: 100%;
        padding: 5px;
        margin: 3px 0 10px 0;
        display: inline-block;
        border: none;
        background: #f1f1f1;
        font-size: 14px;
    }

    label {
        font-weight: 600;
    }

    input[type='radio'] {
    -webkit-appearance:none;
    width:18px;
    height:18px;
    border:1px solid darkgray;
    border-radius:50% !important;
    outline:none;
    box-shadow:0 0 5px 0px gray inset;
}
input[type='radio']:hover {
    box-shadow:0 0 5px 0px orange inset;
}
input[type='radio']:before {
    content:'';
    display:block;
    width:50%;
    height:50%;
    margin: 25% auto;    
    border-radius:50%;    
}
input[type='radio']:checked:before {
    background:black;
}

</style>

