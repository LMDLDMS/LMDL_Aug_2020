                    <div class="col-sm-2">
                        <input type="text" class="form-control" value="@Model.emp_phone" id="fmobile" name="mobile" maxlength="10" databvalidator="required">
                    </div>

                    <label class="control-label col-sm-2" for="email">Email<span style="color:red;font-size:20px;">*</span>:</label>
                    <div class="col-sm-4">
                        <input type="email" class="form-control" id="email" placeholder="Enter email" name="email" databvalidator="required" value="@Model.emp_personal_email">
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label col-sm-2" for="addr">Address<span style="color:red;font-size:20px;">*</span>:</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="addr" placeholder="Enter Street" name="addr" databvalidator="required">
                    </div>
                </div>

