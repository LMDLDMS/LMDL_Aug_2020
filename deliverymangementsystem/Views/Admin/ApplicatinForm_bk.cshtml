<body>

    <div class="container">
        <div>
            @using (Html.BeginForm("ApplicatinForm", "Admin", FormMethod.Post, new { @class = "form-horizontal", @databvalidatorvalidate = "", @databvalidatortheme = "bs3form", @enctype = "multipart/form-data" }))
