                        <div class="col-sm-2">
                            <input type="radio" name="isconvicted" id="isconvictedyes" value="yes"> Yes
                            <input type="radio" name="isconvicted" id="isconvictedno" value="no"> No
                        </div>

                        <label id="s1" class="control-label col-sm-2" for="convictiondetails">Explain:</label>
                        <div class="col-sm-5" id="s11">
