                        <label class="control-label col-sm-2" for="position">Position Applied for:</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" id="position" value="Delivery Associate" disabled name="position">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="control-label col-sm-2" for="startdate">Available Start Date<span style="color:red;font-size:20px;">*</span>:</label>
                        <div class="col-sm-2">
                            <input type="text" class="form-control" id="startdate" name="startdate" databvalidator="required">
                        </div>

                        <label class="control-label col-sm-2" for="desiredpay">Desired Pay<span style="color:red;font-size:20px;">*</span>:</label>
                        <div class="col-sm-2">
                            <input type="number" class="form-control" id="desiredpay" name="desiredpay" databvalidator="number,required">
                        </div>

                        <label class="control-label col-sm-2">Employment Desired?<span style="color:red;font-size:20px;">*</span></label>
                        <div class="col-sm-2">
                            <input type="radio" name="employmentdesired" value="fulltime"> Full Time
