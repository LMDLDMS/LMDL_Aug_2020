@model deliverymangementsystem.EDM.tblrescue
<div class="outer-w3-agile mt-3">
    <h4 class="tittle-w3-agileits mb-4">Rescue Details</h4>
    @using (Html.BeginForm("RescueDetails", "RouteTracking", FormMethod.Post, new { @id = "form1", @databvalidatorvalidate = "", @databvalidatortheme = "bs3form" }))
    {

        <div class="form-group">
            <label for="inputAddress2">Rescue Details</label>
            <div class="d-block my-3">

                <div class="form-group">
                    @Html.TextBoxFor(c => c.rescue_package, new { @class = "form-control", @placeholder = "Rescue Package" })

                </div>
                <div class="form-group">
                    @Html.TextBoxFor(c => c.rescue_stops, new { @class = "form-control", @placeholder = "Rescue Stops" })

                </div>
                <div class="form-group">
                    @Html.TextBoxFor(c => c.first_package_time, new { @class = "form-control", @placeholder = "Rescue First Package Time" })

                </div>
                <div class="form-group">
                    @Html.TextBoxFor(c => c.last_package_time, new { @class = "form-control", @placeholder = "Rescue Last Package Time" })

                </div>
                <div class="form-group">

                    @Html.TextBoxFor(c => c.route, new { @class = "form-control", @placeholder = "Enter Route Details"})

                </div>
                <div class="form-group">

                    @Html.TextAreaFor(c => c.rescue_notes, new { @class = "form-control", @placeholder = "Rescue notes" })

                </div>



            </div>
        </div>

        
        <button type="submit" class="btn btn-primary">Submit</button>
    }

</div>
