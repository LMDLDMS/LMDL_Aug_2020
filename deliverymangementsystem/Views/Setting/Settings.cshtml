<script type="text/javascript">

    function HideShowMoreAddress(dvID, Action) {
        alert(dvID + Action);
        if (Action == 'Hide') {
            $(dvID).css('display', 'none');
        }
        else {
            $(dvID).css('display', 'block');
        }
    }

    function DeleteImage(ID, hdnID) {
        if (confirm('Do you want to delete image ?')) {
            //alert(ID);
            //document.getElementById('hdnImg2').value = '';
            //alert(hdnID.value);
            $(ID).attr('src', '');
            //$(hdnID).value = '';
            document.getElementById(hdnID).value = '';
            //alert(hdnID.value);
        }
    }

    function ReadFile(input, ID) {
        //alert(ID);
        //alert(input);
        if (input.files && input.files[0]) {
            var reader = new FileReader();
            reader.onload = function (e) {
                //alert(e.target.result);
                $(ID).attr('src', e.target.result);
            }
            reader.readAsDataURL(input.files[0]);
        }
    }

    //function openPage(pageName, elmnt, clr) {
    //    //alert('#' + pageName);
    //    var i, tabcontent, tablinks;
