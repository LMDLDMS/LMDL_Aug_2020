    //    for (i = 0; i < tablinks.length; i++) {
    //        tablinks[i].style.backgroundColor = "";
    //        tablinks[i].style.color = clr;
    //    }
    //    document.getElementById(pageName).style.display = "block";
    //    elmnt.style.backgroundColor = clr;
    //    elmnt.style.color = 'white';
    //}

    $(document).ready(function () {
        // Get the element with id="defaultOpen" and click on it
        //document.getElementById("defaultOpen").click();
        //document.getElementById("defaultOpen").backgroundColor = "gray";

        //$("#CompLogo").change(function () {
        //    //alert('COmpLogo');
        //    var ID = '#CompLogoImg';
        //    ReadFile(this, ID);
        //});

        //$("#SiteIcon").change(function () {
        //    //alert('SiteLogo');
        //    var ID = '#SiteIconImg';
        //    ReadFile(this, ID);
        //});

        $("#LoginLogo").change(function () {
            //alert('COmpLogo');
            var ID = '#LoginLogoImg';
            ReadFile(this, ID);
        });

        $("#BgImage").change(function () {
            //alert('SiteLogo');
            var ID = '#BackgroundImage';
            ReadFile(this, ID);
        });



        var TimeZone = $('#hdnTimeZone').val();
        
        if (TimeZone.trim() == '') {
            $('#TimeZone').val('America/New_Jersey')
        }
        else {
            $('#TimeZone').val(TimeZone)
        }

        var DateFormat = $('#hdnDateFormat').val();
        if (DateFormat.trim() == '') {
            $('#DateFormat').val('"MM/dd/yyyy"');
        }
        else {
            $('#DateFormat').val(DateFormat);
        }

        var TimeFormat = $('#hdnTimeFormat').val();
        if (TimeFormat.trim() == '') {
            $('#TimeFormat').val('"hh:mm tt"');
        }
        else {
            $('#TimeFormat').val(TimeFormat);
        }

        var WeekStart = $('#hdnWeekStart').val();
        $('#WeekStart').val(WeekStart);

        var Position = $('#hdnPosition').val();
        $('#Position').val(Position);

        var ForgotPassLink = $('#hdnForgotPassLink').val();
        $('#ForgotPassLink').val(ForgotPassLink);

        var BackToHomeLink = $('#hdnBackToHomeLink').val();
        $('#BackToHomeLink').val(BackToHomeLink);

        //$('#dvAdd2').hide();
        //$('#dvAdd3').hide();

        //binds to onchange event of your input field
        $('#CompLogo').bind('change', function () {
            var fileSize = this.files[0].size;
            // 1048576
            fileSize = fileSize / 1048576;
            if (fileSize > 2) {
                alert("Maximum Company Logo size is 2 MB. \nPlease upload image below or equal to 2 MB.\nYour Company Logo size is - " + parseInt(fileSize) + " MB.");
                $('#CompLogo').val('');
                //$('#CompLogoImg').val('');
                $('#CompLogoImg').attr("src", "");
            }
            else {
                var ID = '#CompLogoImg';
                ReadFile(this, ID);
            }

        });

        $('#SiteIcon').bind('change', function () {
            var fileSize = this.files[0].size;
            // 1048576
            fileSize = fileSize / 1048576;
            if (fileSize > 0.5) {
                alert("Maximum Site icon size is 500 KB. \nPlease upload image below or equal to 500 KB.\nYour Site icon size is - " + parseInt(fileSize) + " MB.");
                $('#SiteIcon').val('');
                //$('#SiteIconImg').val('');
                $('#SiteIconImg').attr("src", "");
            }
            else {
                var ID = '#SiteIconImg';
                ReadFile(this, ID);
            }

        });

    });

</script>




